<style>
    /* Section about us */
    .aoutus {
        padding: 50px 0;
        background-color: white;
    }

        .aoutus .row {
            display: flex;
            align-items: center;
        }

        .aoutus .imgg {
            width: 100%;
            height: auto;
            border-radius: 10px;
            box-shadow: 0 4px 8px rgba(0,0,0,0.1);
        }

        .aoutus .des {
            list-style: none;
            padding: 0;
            margin: 0;
        }

            .aoutus .des li {
                font-size: 18px;
                margin-bottom: 15px;
                line-height: 1.5;
            }

    /* Section testimonials */
    .testimonials-section {
        padding: 50px 0;
        background-color: #fff;
        text-align: center;
    }

        .testimonials-section h2 {
            font-size: 32px;
            margin-bottom: 30px;
            font-weight: bold;
        }

        .testimonials-section .row {
            display: flex;
            justify-content: space-around;
        }

        .testimonials-section blockquote {
            border-left: 5px solid #ff6600;
            padding-left: 20px;
            margin: 20px;
            font-style: italic;
            font-size: 18px;
            position: relative;
        }

            .testimonials-section blockquote::before {
                content: "\201C";
                font-size: 40px;
                position: absolute;
                left: -10px;
                top: -10px;
                color: #ff6600;
            }

        .testimonials-section cite {
            display: block;
            margin-top: 15px;
            font-size: 16px;
            color: #555;
        }

        .testimonials-section img {
            width: 50px;
            height: 50px;
            border-radius: 50%;
            margin-right: 10px;
            vertical-align: middle;
        }

    /* Section sponsors */
    .sponsors-section {
        padding: 50px 0;
        background-color: white;
        text-align: center;
    }

        .sponsors-section h2 {
            font-size: 32px;
            margin-bottom: 30px;
            font-weight: bold;
        }

    .sponsor-showcase {
        display: flex;
        justify-content: space-around;
        list-style: none;
        padding: 0;
        margin: 0;
        flex-wrap: wrap; /* Allow wrapping on smaller screens */
    }

        .sponsor-showcase li {
            display: inline-block;
            margin: 0 15px;
            text-align: center;
        }

        .sponsor-showcase figure {
            margin: 0;
            overflow: hidden;
        }

        .sponsor-showcase img.logo-sponsor {
            width: 300px;
            height: auto;
            display: block;
            margin: 0 auto 10px;
            transition: transform 0.3s ease; /* Smooth zoom effect */
        }

            .sponsor-showcase img.logo-sponsor:hover {
                transform: scale(1.2); /* Zoom in effect */
            }

        .sponsor-showcase figcaption {
            font-size: 16px;
            color: #333; /* Changed to a darker color for better contrast */
            font-weight: bold;
            margin-top: 10px;
        }

</style>

